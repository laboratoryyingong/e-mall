{{!-- app/templates/components/product-listing --}}

<div class="card">
    <LinkTo @route="products.show" @model={{this.product}}>
        <img class="card-img-top"
            src="data:image/jpeg;base64,"
            alt="Card image cap">
    </LinkTo>
    <div class="card-body">
        <h5 class="card-title">{{this.product.name}}</h5>
        <p class="card-text">price : {{this.product.price}}</p>
        <p class="card-text">{{this.product.intro}}</p>
    </div>
    <p class="card-footer">
        <button type="button" class="btn btn-outline-primary">Add to Cart</button>
        <LinkTo @route="products.show" @model={{this.product}} class="btn btn-primary">
            View Details
        </LinkTo>
    </p>
</div>